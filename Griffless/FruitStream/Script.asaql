SELECT
    *, CASE WHEN name = 'Banana' THEN 'Yes'
    ELSE 'No' END AS FruitEatenByMonkeys
INTO
    [fruitlake]
FROM
    [fruitstream]

SELECT
    [name] AS FruitName
    ,COUNT(*) AS FruitCount
INTO
    [fruitreport]
FROM
    [fruitstream]
GROUP BY 
    [name],
    SlidingWindow(second,10)

SELECT 
    [name], [colour], [price]
INTO 
    [FruitDB]
FROM
    [fruitstream]